{
  "name": "@workwork/monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "Env",
    "server",
    "browser",
    "packages/*"
  ],
  "scripts": {
    "dev:server": "npm run dev --workspace=@app/server",
    "dev:browser": "npm run dev --workspace=@app/browser",
    "build:server": "npm run build --workspace=@app/server",
    "build:browser": "npm run build --workspace=@app/browser"
  },
  "dependencies": {
    "@workwork/env": "file:./Env"
  }
}
