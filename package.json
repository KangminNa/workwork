{
  "name": "workwork-monorepo",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev:web": "pnpm --filter web dev",
    "dev:api": "pnpm --filter api dev",
    "dev": "concurrently \"pnpm:dev:web\" \"pnpm:dev:api\"",
    "start:web": "pnpm --filter web start",
    "start:api": "pnpm --filter api start",
    "build": "tsc --build",
    "lint": "pnpm -r lint",
    "prisma": "dotenv -e infra/config/.env -- pnpm prisma"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.19.0",
    "typescript": "^5.0.0"
  }
}
