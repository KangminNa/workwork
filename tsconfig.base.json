{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "rootDir": "./",
    "baseUrl": ".",
    "paths": {
      "@workwork/base": ["base"],
      "@workwork/base/*": ["base/*"],
      "@workwork/auth": ["auth"],
      "@workwork/auth/*": ["auth/*"]
    },
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
