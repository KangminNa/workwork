{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@base/browser/*": [
        "base/browser/*"
      ],
      "@base/server/*": [
        "base/server/*"
      ],
      "@base/shared/*": [
        "base/shared/*"
      ]
    }
  }
}
