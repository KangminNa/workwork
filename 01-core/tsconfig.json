{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "../../dist/01-core",
    "rootDir": "src",
    "composite": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "tsBuildInfoFile": "../../dist/01-core/.tsbuildinfo"
  },
  "include": ["src"]
}
